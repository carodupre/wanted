<%= simple_form_for [booking, review] do |f| %>
  <%= f.input :title %>
  <%= f.input :rating, as: :select, collection: 0..5 %>
  <%= f.input :description %>
<!--   <#%= f.input :photo %> if we had the photo later in the migration
  <%#= f.input :photo_cache, as: :hidden %> if we had the photo later in the migration -->
  <%= f.button :submit %>
<% end %>

